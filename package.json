  "author": "HearHellacopters",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hearhellacopters/hash-maker/issues"
  },
  "homepage": "https://github.com/hearhellacopters/hash-maker#readme",
  "devDependencies": {
    "@types/mocha": "^10.0.2",
    "@types/node": "^20.7.0",
    "@typescript/lib-dom": "npm:@types/web@^0.0.115",
    "typescript": "^5.2.2"
  },
  "engines": {
